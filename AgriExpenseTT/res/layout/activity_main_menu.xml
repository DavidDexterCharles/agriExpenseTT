<uwi.dcit.AgriExpenseTT.helpers.FlyOutContainer xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menuContain"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
	<LinearLayout
	    android:id="@+id/sidemenu_mainmennu"
	    android:layout_height="match_parent"
	    android:layout_width="300dp"
	    android:orientation="vertical"/>
 
    <LinearLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="uwi.dcit.AgriExpenseTT.MainMenu"
        tools:ignore="MergeRootFrame" >
		
        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
            android:adjustViewBounds="true"
            android:contentDescription="@string/image_description_agrinett_logo"
            android:maxWidth="180dp"
            android:src="@drawable/logo_agrinet" />
        	
    	<ScrollView
    		android:id="@+id/scrollView1"
    		android:layout_width="match_parent"
    		android:layout_height="match_parent" >    
           	
           	<LinearLayout
	        	android:layout_width="match_parent"
	       	 	android:layout_height="wrap_content"
	        	android:orientation="vertical">  
	   
		         <Button
		            android:id="@+id/NewPurchase"
		            android:layout_width="@dimen/home_btn_width"
		            android:layout_height="@dimen/home_btn_height"
		            android:layout_gravity="center_horizontal"
		            android:background="@drawable/btn_custom"
		            android:layout_marginTop="@dimen/home_btn_magrin"
		            android:text="@string/btn_new_purchase"
		            android:onClick="openNewPurchaseFragment" />
	
				<Button
				    android:id="@+id/newCycle"
				    android:layout_width="@dimen/home_btn_width"
				    android:layout_height="@dimen/home_btn_height"		
				    android:layout_gravity="center_horizontal"
				    android:background="@drawable/btn_custom"
				    android:layout_marginTop="@dimen/home_btn_magrin"	
				    android:text="@string/btn_new_cycle" 
				    android:onClick="openNewCycleFragment"/>

				<Button
				    android:id="@+id/HireLabour"
				    android:layout_width="@dimen/home_btn_width"
				    android:layout_height="@dimen/home_btn_height"
				    android:layout_gravity="center_horizontal"
				    android:background="@drawable/btn_custom"
				    android:layout_marginTop="@dimen/home_btn_magrin"
				    android:text="@string/btn_new_labour"
				    android:onClick="openManageLabourFragment" />

				<Button
				    android:id="@+id/ResDetail"
				    android:layout_width="@dimen/home_btn_width"
				    android:layout_height="@dimen/home_btn_height"
				    android:layout_gravity="center_horizontal"
				    android:background="@drawable/btn_custom"
				    android:layout_marginTop="@dimen/home_btn_magrin"
				    android:text="@string/btn_man_resource" 
				    android:onClick="openManageResourceFragment"/>

				<Button
				    android:id="@+id/manageData"
				    android:layout_width="@dimen/home_btn_width"
				    android:layout_height="@dimen/home_btn_height"		
				    android:layout_gravity="center_horizontal"
				    android:background="@drawable/btn_custom"
				    android:layout_marginTop="@dimen/home_btn_magrin"	
				    android:text="@string/btn_man_data"
				    android:onClick="openManageDataFragment" />

				 <Button
				    android:id="@+id/generateFile"
				    android:layout_width="@dimen/home_btn_width"
				    android:layout_height="@dimen/home_btn_height"
				    android:layout_gravity="center_horizontal"
				    android:background="@drawable/btn_custom"
				    android:layout_marginTop="@dimen/home_btn_magrin"
				    android:text="@string/btn_gen_report"
				    android:onClick="openManageExports" />
				 
				<Button
				    android:id="@+id/btn_SignIn"
				    android:layout_width="@dimen/home_btn_width"
				    android:layout_height="@dimen/home_btn_height"
				    android:layout_gravity="center_horizontal"
				    android:background="@drawable/btn_custom"
				    android:layout_marginTop="@dimen/home_btn_magrin"
				    android:text="@string/btn_man_signin"
				    android:onClick="signInStart" />


				<Button
				    android:id="@+id/btn_About"
				    android:layout_width="@dimen/home_btn_width"
				    android:layout_height="@dimen/home_btn_height"
				    android:layout_gravity="center_horizontal"
				    android:background="@drawable/btn_custom"
				    android:layout_marginTop="@dimen/home_btn_magrin"
				    android:text="@string/btn_man_about"
				    android:onClick="openAboutFragment" />
	      
			</LinearLayout>
		</ScrollView>
    
    </LinearLayout>

    

       
   

</uwi.dcit.AgriExpenseTT.helpers.FlyOutContainer>
